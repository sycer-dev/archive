{
	"name": "success-poster",
	"version": "4.0.0",
	"description": "the Sycer Development success poster.",
	"main": "dist/index.js",
	"scripts": {
		"lint": "eslint src --ext .ts",
		"lint:fix": "eslint src --fix --ext .ts --debug",
		"cleardir": "rimraf build/*",
		"compile": "tsc",
		"build": "pnpm run cleardir && pnpm run compile",
		"start": "pnpm run build && node ."
