{
	"name": "@sycer/carts",
	"version": "1.0.0",
	"description": "a file structure for a discord-akairo based discord bot",
	"private": true,
	"main": "dist/index.js",
	"license": "MIT",
	"scripts": {
		"lint": "eslint src --ext .ts",
		"lint:fix": "eslint src --fix --ext .ts",
		"cleardir": "rimraf dist/*",
		"compile": "tsc",
		"build": "pnpm run cleardir && pnpm run compile",
		"start": "pnpm run build && node ."
	},
	"author": "Carter Himmel <me@fyko.net>",
	"dependencies": {
		"@naval-base/ms": "^3.1.0",
		"@types/common-tags": "^1.8.0",
		"common-tags": "^1.8.0",
		"discord-akairo": "^8.0.0",
		"discord.js": "^12.2.0",
		"mongoose": "^5.9.25",
		"node-emoji": "^1.10.0",
		"tslib": "^2.0.0",
		"winston": "^3.3.3"
	},
	"devDependencies": {
		"@types/mongoose": "^5.7.32",
		"@types/node": "^14.0.23",
		"@types/node-emoji": "^1.8.1",
		"@types/ws": "^7.2.6",
		"@typescript-eslint/eslint-plugin": "^3.6.1",
		"@typescript-eslint/parser": "^3.6.1",
		"eslint": "^7.4.0",
		"eslint-config-marine": "^7.1.2",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-prettier": "^3.1.4",
		"prettier": "^2.0.5",
		"rimraf": "^3.0.2",
		"ts-node": "^8.10.2",
		"typescript": "^3.9.7"
	},
	"eslintConfig": {
		"extends": "marine/prettier/node",
		"rules": {
			"comma-dangle": [
				"error",
				"always-multiline"
			],
			"@typescript-eslint/naming-convention": "off",
			"@typescript-eslint/restrict-template-expressions": "off"
		}
	}
}
